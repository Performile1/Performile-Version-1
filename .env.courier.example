# COURIER API CREDENTIALS
# Copy this file to .env.courier and fill in your actual credentials

# PostNord API
POSTNORD_API_KEY=your_api_key_here
POSTNORD_CUSTOMER_NUMBER=your_customer_number_here

# Bring API
BRING_API_KEY=your_api_key_here
BRING_CUSTOMER_NUMBER=your_customer_number_here

# Budbee API
BUDBEE_API_KEY=your_api_key_here
BUDBEE_API_SECRET=your_api_secret_here

# API Base URLs
POSTNORD_BASE_URL=https://api2.postnord.com
BRING_BASE_URL=https://api.bring.com
BUDBEE_BASE_URL=https://api.budbee.com
